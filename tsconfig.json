{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
  "baseUrl": ".",
  "allowJs": true,
  "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
  "**/*.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
